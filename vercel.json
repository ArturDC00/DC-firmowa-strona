{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/checklista-software-house", "destination": "/checklista-software-house.html" },
    { "source": "/checklista", "destination": "/checklista-software-house.html" },
    { "source": "/kalkulatory", "destination": "/kalkulatory.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}